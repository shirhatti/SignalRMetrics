{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "library": "@microsoft/signalr@latest",
      "destination": "wwwroot/js/signalr/",
      "files": [
        "dist/browser/signalr.min.js",
        "dist/browser/signalr.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "Chart.js@2.9.3",
      "destination": "wwwroot/lib/Chart.js/",
      "files": [
        "Chart.bundle.js",
        "Chart.bundle.min.js",
        "Chart.css"
      ]
    }
  ]
}